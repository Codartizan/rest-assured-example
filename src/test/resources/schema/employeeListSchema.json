{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "status",
    "data"
  ],
  "properties": {
    "status": {
      "$id": "#/properties/status",
      "type": "string",
      "title": "The Status Schema",
      "default": "",
      "examples": [
        "success"
      ],
      "pattern": "^(.*)$"
    },
    "data": {
      "$id": "#/properties/data",
      "type": "array",
      "title": "The Data Schema",
      "items": {
        "$id": "#/properties/data/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "id",
          "employee_name",
          "employee_salary",
          "employee_age",
          "profile_image"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/data/items/properties/id",
            "type": "string",
            "title": "The Id Schema",
            "default": "",
            "examples": [
              "1"
            ],
            "pattern": "^(.*)$"
          },
          "employee_name": {
            "$id": "#/properties/data/items/properties/employee_name",
            "type": "string",
            "title": "The Employee_name Schema",
            "default": "",
            "examples": [
              "Tiger Nixon"
            ],
            "pattern": "^(.*)$"
          },
          "employee_salary": {
            "$id": "#/properties/data/items/properties/employee_salary",
            "type": "string",
            "title": "The Employee_salary Schema",
            "default": "",
            "examples": [
              "320800"
            ],
            "pattern": "^(.*)$"
          },
          "employee_age": {
            "$id": "#/properties/data/items/properties/employee_age",
            "type": "string",
            "title": "The Employee_age Schema",
            "default": "",
            "examples": [
              "61"
            ],
            "pattern": "^(.*)$"
          },
          "profile_image": {
            "$id": "#/properties/data/items/properties/profile_image",
            "type": "string",
            "title": "The Profile_image Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}